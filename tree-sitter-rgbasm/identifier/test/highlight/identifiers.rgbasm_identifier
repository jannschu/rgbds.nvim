my_var
; <- variable
;^^^^^ variable

; Global constant
ACCLATCH0_START
; <- constant
;^^^^^^^^^^^^^^ constant

; Local label
.local_lbl
; <- punctuation.delimiter
;^^^^^^^^^^^^ label

; Qualified identifier
my_module.local_lbl
; <- module
;        ^ punctuation.delimiter
;         ^^^^^^^^^ label

; Interpolation
{my_var}
; <- punctuation.special.interpolation
;^^^^^^ variable
;      ^ punctuation.special.interpolation

; Interpolation with format
{x:my_var}
; <- punctuation.special.interpolation
;^ string.escape
; ^ punctuation.delimiter
;  ^^^^^^ variable
;        ^ punctuation.special.interpolation

; Raw identifier
#raw
; <- punctuation.delimiter
;^^^ variable

; Complex interpolation
my_{var}_name
;  ^ punctuation.special.interpolation
;   ^ variable
;      ^ punctuation.special.interpolation

; Qualified with interpolation
mod_{part}.loc_{part}
; <- module
;   ^ punctuation.special.interpolation
;    ^^^^ variable
;        ^ punctuation.special.interpolation
;         ^ punctuation.delimiter
;          ^^^^ label
;              ^ punctuation.special.interpolation
;               ^^^^ variable
;                   ^ punctuation.special.interpolation

; Interpolation with constant
{ACCLATCH0_START}
; <- punctuation.special.interpolation
;^^^^^^^^^^^^^^^ constant
;               ^ punctuation.special.interpolation
