{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rgbasm",
      "camelcase": "RgbAsm",
      "title": "RGBDS - Game Boy assembly",
      "scope": "source.rgbasm",
      "file-types": [
        "rgbasm"
      ],
      "injection-regex": "^rgbasm$",
      "class-name": "TreeSitterRgbasm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "RGBDS assembly grammar for tree-sitter",
    "authors": [
      {
        "name": "Jannik Sch√ºrg",
        "email": "jannik.schuerg@posteo.de"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-rgbasm"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
